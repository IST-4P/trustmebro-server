{
  "ConnectionStrings": {
    "DefaultConnection": "Host=${DB_HOST};Port=${DB_PORT};Database=${DB_NAME_REVIEW};Username=${DB_USERNAME};Password=${DB_PASSWORD};SSL Mode=Disable"
  },
  "Kafka": {
    "BootstrapServers": "localhost:29092",
    "ClientId": "review-service",
    "GroupId": "review-service-group"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http2"
    },

    "Keycloak": {
      "Authority": "https://keycloak-trustmebro.hacmieu.xyz/realms/TrustMeBro",
      "Audience": "auth"
    }
  }
}
