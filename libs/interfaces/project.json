{
  "name": "interfaces",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/interfaces/src",
  "projectType": "library",
  "tags": [],
  "// targets": "to see all targets run: nx show project interfaces --web",
  "targets": {
    "generate-ts-proto": {
      "command": "npx protoc --plugin=protoc-gen-ts_proto=..\\..\\..\\..\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=./proto-types --proto_path=./protos --ts_proto_opt=nestJs=true ./protos/*.proto",
      "options": {
        "cwd": "{projectRoot}/src/lib",
        "input": ["protos/*.proto"]
      },
      "cache": true
    }
  }
}
